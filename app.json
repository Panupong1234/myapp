{
  "expo": {
    "name": "react native tutorial",
    "slug": "react_native_tutorial",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_a.png",
    "splash": {
      "image": "./assets/splash_2.png",
      "resizeMode": "contain",
      "backgroundColor": "#673AB7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appstoreicon.png",
        "backgroundColor": "#673AB7"
      },
      "package": "con.example.myapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
